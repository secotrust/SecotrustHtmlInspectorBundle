{% extends '@WebProfiler/Profiler/layout.html.twig' %}

{% block toolbar %}
    {% set text %}
    Open the console to view the HTML Inspector output, click to repeat the inspection.
    {% endset %}
    {% set icon %}
    <a href="#" id="html-inspector">
    <img onload="(function(){ var js = document.createElement('script'); js.src = '{{ path('secotrust_html_inspector_js') }}'; document.getElementsByTagName('body')[0].appendChild(js);})();" width="20" height="28" alt="HTML Inspector" src="data:image/png;base64," />
    <span id="html-inspector-count" class="sf-toolbar-status">?</span>
    </a>
    {% endset %}
    {% include '@WebProfiler/Profiler/toolbar_item.html.twig' with { 'link': false } %}
{% endblock %}
